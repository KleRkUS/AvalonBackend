{
  "name": "avalon_online",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.ts",
  "module": "dist/index.min.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "nodemon",
    "start": "node dist/index.js",
    "start:build": "node build/index.js",
    "lint": "npx eslint src",
    "lint:fix": "npx eslint --fix src",
    "clean": "rm -fr build",
    "build": "npm run clean && npm run lint && npx ts-node -i -r tsconfig-paths/register tsconfig.build.json && npm run bundle:esm && npm run bundle:esm:min",
    "bundle:esm": "rollup build/index.js --file build/index.mjs --format esm",
    "bundle:esm:min": "terser --ecma 6 --compress --mangle --module -o build/index.min.mjs -- build/index.mjs && gzip -9 -c build/index.min.mjs > build/index.min.mjs.gz",
    "husky:prepare": "husky install && npx husky add .husky/pre-commit \"npm run lint && npm run prettify\"",
    "prettify": "npx prettier --write ./src"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "config": "^3.3.9",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "rollup": "^3.16.0",
    "sequelize": "^6.28.0",
    "terser": "^5.16.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5",
    "ws": "^8.12.1"
  },
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/ws": "^8.5.4"
  }
}
